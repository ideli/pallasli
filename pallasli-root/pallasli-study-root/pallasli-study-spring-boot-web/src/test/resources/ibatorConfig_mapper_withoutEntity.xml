<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE generatorConfiguration  
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"  
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <classPathEntry location="/Users/lyt1987/Desktop/study/01_maven/repo/mysql/mysql-connector-java/5.1.10/mysql-connector-java-5.1.10.jar" />  

	<context id="sysGenerator" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Example" />
		</plugin>
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://localhost:3306/archive?useUnicode=true&amp;characterEncoding=utf8"
			userId="root" password="root">
		</jdbcConnection>
		<javaModelGenerator targetPackage="com.pallasli.application.model"
			targetProject="${output.dir}">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="sqlmap.activiti"
			targetProject="${output.dir}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<javaClientGenerator type="XMLMAPPER"
			targetPackage="com.pallasli.application.dao" targetProject="${output.dir}"> 
			<property name="enableSubPackages" value="true" />
			<property name="" value=""/>
		</javaClientGenerator>
		 

		<table tableName="t_act_menu" domainObjectName="ActMenu" enableCountByExample="true"
			enableUpdateByExample="true" enableDeleteByExample="true"
			enableSelectByExample="true" selectByExampleQueryId="true">
			<generatedKey column="id" sqlStatement="select uuid()"
				identity="true" />
		</table> 
	</context>
</generatorConfiguration>  